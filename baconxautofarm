local function BLPYDCE_fake_script() -- Frame.Tween 
	local script = Instance.new('LocalScript', Frame)

	wait(6)
	
	local frame = script.Parent
	
	frame.Visible = true
	
	frame:TweenPosition(UDim2.new(0.257, 0,0.196, 0))
end
coroutine.wrap(BLPYDCE_fake_script)()
local function SPYOPHP_fake_script() -- Pfp.LocalScript 
	local script = Instance.new('LocalScript', Pfp)

	local Players = game:GetService("Players")
	
	local player = Players.LocalPlayer
	
	local userId = player.UserId
	local thumbType = Enum.ThumbnailType.HeadShot
	local thumbSize = Enum.ThumbnailSize.Size420x420
	local content, isReady = Players:GetUserThumbnailAsync(userId, thumbType, thumbSize)
	
	local imageLabel = script.Parent
	imageLabel.Image = content
end
coroutine.wrap(SPYOPHP_fake_script)()
