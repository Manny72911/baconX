AntiKick.BackgroundColor3 = Color3.fromRGB(45, 45, 45)
AntiKick.BorderSizePixel = 0
AntiKick.Size = UDim2.new(0, 200, 0, 50)
AntiKick.Font = Enum.Font.SourceSans
AntiKick.Text = "Anti-Kick"
AntiKick.TextColor3 = Color3.fromRGB(255, 255, 255)
AntiKick.TextScaled = true
AntiKick.TextSize = 14.000
AntiKick.TextWrapped = true
AntiKick.MouseButton1Down:Connect(function()
	local a = getrawmetatable(game)
	local sucks = a.__namecall
	local player = game.Players.LocalPlayer
	setreadonly(a, false)
	a.__namecall = newcclosure(function(name, ...)
		local tabs = {
			...
		}
		if getnamecallmethod() == "FireServer" and tostring(name) == "MainEvent" then
			if tabs[1] == "CHECKER_1" or tabs[1] == "TeleportDetect" or tabs[1] == "OneMoreTime" then
				return wait(9e9)
			end
		end
		if getnamecallmethod() == "Kick" then
			return wait(9e9)
		end
		return sucks(name, unpack(tabs))
	end)
	setreadonly(a, true)
end)

